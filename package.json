{
  "name": "vaishali-polling-agent-app",
  "version": "1.0.0",
  "description": "Cross-platform polling agent management app for Vaishali constituency with real-time incident reporting, hierarchical data structure, and Netlify-compatible web deployment.",
  "main": "App.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo export:web",
    "build:netlify": "expo export:web && cp -r web-build/* dist/",
    "eject": "expo eject"
  },
  "dependencies": {
    "expo": "~49.0.15",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-web": "~0.19.6",
    "@expo/vector-icons": "^13.0.0",
    "react-navigation": "^4.4.4",
    "react-navigation-stack": "^2.10.4",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-screens": "~3.22.0",
    "react-native-safe-area-context": "4.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "expo-camera": "~13.4.4",
    "expo-media-library": "~15.4.1",
    "expo-image-picker": "~14.3.2",
    "expo-file-system": "~15.4.4",
    "firebase": "^9.23.0",
    "@react-native-async-storage/async-storage": "1.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true,
  "homepage": ".",
  "repository": {
    "type": "git",
